<context-collection>
  <context id="mass">
    <includes>
      <context-ref>builtins/real-numbers</context-ref>
    </includes>
    <sorts>
      <sort id="ℝp" />
      <sort id="mass" />
    </sorts>
    <subsorts />
    <vars>
      <var id="F" sort="ℝp" />
      <var id="F2" sort="ℝp" />
      <var id="F1" sort="ℝp" />
      <var id="M1" sort="mass" />
      <var id="M" sort="mass" />
      <var id="M2" sort="mass" />
    </vars>
    <ops>
      <op id="_+">
        <arity>
          <sort id="mass" />
          <sort id="mass" />
        </arity>
        <sort id="mass" />
      </op>
      <op id="_÷">
        <arity>
          <sort id="mass" />
          <sort id="ℝp" />
        </arity>
        <sort id="mass" />
      </op>
      <op id="_÷">
        <arity>
          <sort id="mass" />
          <sort id="mass" />
        </arity>
        <sort id="ℝp" />
      </op>
      <op id="_×">
        <arity>
          <sort id="ℝp" />
          <sort id="mass" />
        </arity>
        <sort id="mass" />
      </op>
    </ops>
    <rules>
      <rule>
        <vars />
        <pattern>
          <term op="_+">
            <term op="_×">
              <term-or-var name="F1" />
              <term-or-var name="M" />
            </term>
            <term op="_×">
              <term-or-var name="F2" />
              <term-or-var name="M" />
            </term>
          </term>
        </pattern>
        <condition />
        <replacement>
          <term op="_×">
            <term op="_+">
              <term-or-var name="F1" />
              <term-or-var name="F2" />
            </term>
            <term-or-var name="M" />
          </term>
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_×">
            <term-or-var name="F1" />
            <term op="_×">
              <term-or-var name="F2" />
              <term-or-var name="M" />
            </term>
          </term>
        </pattern>
        <condition />
        <replacement>
          <term op="_×">
            <term op="_×">
              <term-or-var name="F1" />
              <term-or-var name="F2" />
            </term>
            <term-or-var name="M" />
          </term>
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term-or-var name="M" />
            <term-or-var name="F" />
          </term>
        </pattern>
        <condition />
        <replacement>
          <term op="_×">
            <term op="_÷">
              <integer value="1" />
              <term-or-var name="F" />
            </term>
            <term-or-var name="M" />
          </term>
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term-or-var name="M1" />
            <term op="_×">
              <term-or-var name="F" />
              <term-or-var name="M2" />
            </term>
          </term>
        </pattern>
        <condition />
        <replacement>
          <term op="_÷">
            <term op="_÷">
              <term-or-var name="M1" />
              <term-or-var name="F" />
            </term>
            <term-or-var name="M2" />
          </term>
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term op="_×">
              <term-or-var name="F" />
              <term-or-var name="M1" />
            </term>
            <term-or-var name="M2" />
          </term>
        </pattern>
        <condition />
        <replacement>
          <term op="_×">
            <term-or-var name="F" />
            <term op="_÷">
              <term-or-var name="M1" />
              <term-or-var name="M2" />
            </term>
          </term>
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term-or-var name="M" />
            <term-or-var name="M" />
          </term>
        </pattern>
        <condition />
        <replacement>
          <integer value="1" />
        </replacement>
      </rule>
    </rules>
    <equations />
  </context>
  <context id="mass-units">
    <includes>
      <context-ref>mass</context-ref>
      <context-ref>boolean/boolean</context-ref>
    </includes>
    <sorts>
      <sort id="mass-unit" />
      <sort id="mass" />
    </sorts>
    <subsorts>
      <subsort subsort="mass-unit" supersort="mass" />
    </subsorts>
    <vars>
      <var id="MU" sort="mass-unit" />
      <var id="MU2" sort="mass-unit" />
      <var id="MU1" sort="mass-unit" />
    </vars>
    <ops>
      <op id="mg">
        <arity />
        <sort id="mass-unit" />
      </op>
      <op id="kg">
        <arity />
        <sort id="mass-unit" />
      </op>
      <op id="g">
        <arity />
        <sort id="mass-unit" />
      </op>
      <op id="_in">
        <arity>
          <sort id="mass" />
          <sort id="mass-unit" />
        </arity>
        <sort id="mass" />
      </op>
    </ops>
    <rules>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term-or-var name="g" />
            <term-or-var name="kg" />
          </term>
        </pattern>
        <condition />
        <replacement>
          <rational value="1/1000" />
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term-or-var name="mg" />
            <term-or-var name="kg" />
          </term>
        </pattern>
        <condition />
        <replacement>
          <rational value="1/1000000" />
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_in">
            <term-or-var name="M" />
            <term-or-var name="MU" />
          </term>
        </pattern>
        <condition />
        <replacement>
          <term op="_×">
            <term op="_÷">
              <term-or-var name="M" />
              <term-or-var name="MU" />
            </term>
            <term-or-var name="MU" />
          </term>
        </replacement>
      </rule>
      <rule>
        <vars />
        <pattern>
          <term op="_÷">
            <term-or-var name="MU1" />
            <term-or-var name="MU2" />
          </term>
        </pattern>
        <condition>
          <term op="¬">
            <term op="_==">
              <term-or-var name="MU2" />
              <term-or-var name="kg" />
            </term>
          </term>
        </condition>
        <replacement>
          <term op="_÷">
            <term op="_÷">
              <term-or-var name="MU1" />
              <term-or-var name="kg" />
            </term>
            <term op="_÷">
              <term-or-var name="MU2" />
              <term-or-var name="kg" />
            </term>
          </term>
        </replacement>
      </rule>
    </rules>
    <equations />
  </context>
</context-collection>